{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "webfrontend-ref.json",
  "description": "Entity references that only accept \"WebFrontend\" entities as target.",
  "type": "string",
  "allOf": [
    { "$ref": "entity-ref.json" },
    {
      "type": "string",
      "pattern": "^webfrontend:[a-z0-9]([a-z0-9-_]*[a-z0-9])?$"
    }
  ],
  "example": "webfrontend:developer-portal"
}
