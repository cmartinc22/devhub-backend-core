{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "mobilemodule-ref.json",
  "description": "Entity references that only accept \"MobileModule\" entities as target.",
  "type": "string",
  "allOf": [
    { "$ref": "entity-ref.json" },
    {
      "type": "string",
      "pattern": "^mobilemodule:[a-z0-9]([a-z0-9-_]*[a-z0-9])?$"
    }
  ],
  "example": "mobilemodule:checkout-android-module"
}
