{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "queue-ref.json",
  "description": "Entity references that only accept \"Queue\" entities as target.",
  "type": "string",
  "allOf": [
    { "$ref": "entity-ref.json" },
    {
      "type": "string",
      "pattern": "^queue:[a-z0-9]([a-z0-9-_]*[a-z0-9])?$"
    }
  ],
  "example": "queue:aws-queue-orders"
}
