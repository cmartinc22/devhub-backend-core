{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "executable-ref.json",
  "description": "Entity references that only accept \"Executable\" entities as target.",
  "type": "string",
  "allOf": [
    { "$ref": "entity-ref.json" },
    {
      "type": "string",
      "pattern": "^executable:[a-z0-9]([a-z0-9-_]*[a-z0-9])?$"
    }
  ],
  "example": "executable:devhub-cli"
}
